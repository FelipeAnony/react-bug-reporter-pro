{
    "name": "react-bug-reporter-pro",
    "version": "1.0.1",
    "type": "module",
    "main": "./dist/index.umd.js",
    "module": "./dist/index.es.js",
    "types": "./dist/index.d.ts",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/FelipeAnony/react-bug-reporter-pro.git"
    },
    "files": [
        "dist"
    ],
    "exports": {
        ".": {
            "import": "./dist/index.es.js",
            "require": "./dist/index.umd.js",
            "types": "./dist/index.d.ts"
        },
        "./dist/style.css": "./dist/style.css"
    },
    "scripts": {
        "dev": "vite",
        "build": "npm run clean && npx tsc --project tsconfig.build.json && vite build",
        "clean": "rimraf dist",
        "lint": "eslint .",
        "preview": "vite preview",
        "lint:fix": "eslint --fix",
        "format": "prettier --write",
        "prepare": "husky"
    },
    "peerDependencies": {
        "react": "^18.0.0",
        "react-dom": "^18.0.0"
    },
    "dependencies": {
        "browser-http-request-listener": "1.0.4",
        "react-icons": "^5.3.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.11.1",
        "@types/node": "^22.7.5",
        "@types/react": "^18.3.10",
        "@types/react-dom": "^18.3.0",
        "@vitejs/plugin-react": "^4.3.2",
        "eslint": "^9.11.1",
        "eslint-plugin-react-hooks": "^5.1.0-rc.0",
        "eslint-plugin-react-refresh": "^0.4.12",
        "globals": "^15.9.0",
        "husky": "^9.1.6",
        "lint-staged": "^15.2.10",
        "prettier": "^3.3.3",
        "rimraf": "^6.0.1",
        "typescript": "^5.5.3",
        "typescript-eslint": "^8.7.0",
        "vite": "^5.4.8",
        "vite-plugin-css-injected-by-js": "^3.5.2",
        "vite-plugin-dts": "^4.2.3"
    },
    "keywords": [
        "react",
        "bug",
        "reporter",
        "library"
    ],
    "author": "Felipe Oliveira Gomes",
    "license": "MIT"
}
